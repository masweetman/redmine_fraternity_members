  <% if @users_by_role.any? %>
  <div class="members box">
    <h3><%=l(:label_member_plural)%></h3>
    <% @users_by_role.keys.sort.each do |role| %>
    	<% if @project.parent_id == 6 && role.name == "Natl President" %>
    		<br>
    	<% end %>
        <span class="label"><%=h role %>:</span> <%= @users_by_role[role].sort.collect{|u| link_to_user u}.join(", ").html_safe %><br>
    <% end %>
  </div>
  <% end %>
